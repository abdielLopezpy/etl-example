{
  "name": "crm-etl",
  "version": "1.0.0",
  "description": "Servicio CrmETL - Sistema de ETL para sincronización de datos de CRM con API CRUD",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:init": "node dist/database/init.js"
  },
  "keywords": [
    "crm",
    "etl",
    "hubspot",
    "api",
    "crud",
    "typescript"
  ],
  "author": {
    "name": "Abdiel López",
    "url": "https://github.com/abdielLopezpy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abdielLopezpy/crm-etl.git"
  },
  "bugs": {
    "url": "https://github.com/abdielLopezpy/crm-etl/issues"
  },
  "homepage": "https://github.com/abdielLopezpy/crm-etl#readme",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "pino": "^8.16.2",
    "pino-http": "^8.5.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nock": "^13.4.0",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
